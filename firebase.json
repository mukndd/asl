{
    "hosting": {
      "public": "asl",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "error_page": {
        "404": "/404.html"
      }
    }
  }
  